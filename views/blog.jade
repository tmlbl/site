doctype
html
  head
    title Blog of Tim Lebel | JavaScript and Node.js Tips and Tutorials
    link(href="data:image/x-icon;base64," rel="icon" type="image/x-icon")
    meta(charset='utf-8')
    meta(http-equiv='Pragma', content='no-cache')
    meta(http-equiv='Expires', content='-1')
    meta(name='apple-mobile-web-app-capable', content='yes')
    meta(name='apple-mobile-web-app-status-bar-style', content='black')
    meta(name='viewport', content='width=device-width')
    meta(name='viewport', content='initial-scale=1.0, maximum-scale=1.0')
    meta(content='minimum-scale=1.0, width=device-width, maximum-scale=1, user-scalable=no' name='viewport')
    meta(name='description' content='Along The Way: Receive hints on places you might like to checkout along the way.')
    meta(name='keywords' content='javascript, node, node.js, express, tutorials, js')
    meta(name='author' content='TimLebel')
  body
    include _header.jade
    each post in posts
      .container
        a(href='/posts/' + post.url)  
          h1.post-title= post.title
        h6.post-date= post.postdate
        p.post-body !{post.body}
        //a.post-link(href='/posts/' + post.url) View Comments
    .container
      a(href='/blog/' + (parseInt(pagenum) + 1)) next page
    include _footer.jade